<!DOCTYPE html>
<html>
<head>
	<title>Hackhouse | Sign Up</title>
	<%= stylesheet_link_tag    'users', media: 'screen, projection' %>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
</head>

<h1 class="testclass">Sign Up:</h1>

<%= form_for :user, url: '/users' do |f| %>

	Name: <%= f.text_field :name %><br />
	Email: <%= f.text_field :email %><br />
	Password: <%= f.password_field :password %><br />
	Password Confirmation: <%= f.password_field :password_confirmation %><br />
	<%= f.submit "Submit" %>

<% end %>